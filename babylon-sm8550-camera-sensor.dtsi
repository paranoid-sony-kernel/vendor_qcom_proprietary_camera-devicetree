	camera_front1_avdd_ldo: gpio-regulator@0 {
		compatible = "regulator-fixed";
		reg = <0x00 0x00>;
		regulator-name = "camera_front1_avdd_ldo";
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
		regulator-enable-ramp-delay = <100>;
		enable-active-high;
		gpio = <&tlmm 180 0>;
		pinctrl-names = "front1_avdd_active", "front1_avdd_suspend";
		pinctrl-0 = <&cam_sensor_front1_active_avdd
					&cam_sensor_front1_suspend_avdd>;
	};

